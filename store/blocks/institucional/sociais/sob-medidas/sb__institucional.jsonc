  "flex-layout.row#institucional-sob-medida__content": {
    "title": "INSTITUCIONAL - Sob Medida",
    "children": ["info-card#sb__info-cards--item1"],
    "props": {
      "blockClass": "institucional-sb__container",
      "colSizing": "auto"
