      "height": "",
      "images": [
        {
          "loading": "lazy",
          "image": "assets/mock/institucional/sb-prefooter-banner.png",
          "mobileImage": "assets/mock/institucional/sb-prefooter-banner-mob.png",
          "description": "banner"
        }
      ]
    }
