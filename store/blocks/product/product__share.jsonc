  },
  "disclosure-content#product__share--content": {
    "props": {
      "blockClass": "product__info--share"
    },
    "children": ["share"]
  },
  "rich-text#product__share--triggertext": {
    "props": {
      "blockClass": "product__info--share",
      "text": "![compartilhar](assets/icons/icon-share.svg)"
    }
  }
}
