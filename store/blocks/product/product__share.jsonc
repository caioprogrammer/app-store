    "props": {
      "blockClass": "product__info--share",
      "animated": true
    },
    "children": [
      "disclosure-trigger#product__share--trigger",
      "disclosure-content#product__share--content"
    ]
  },
  "disclosure-trigger#product__share--trigger": {
    "props": {
      "blockClass": "product__info--share"
    },
